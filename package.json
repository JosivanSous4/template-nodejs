{
  "name": "organization-application-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon src/server.ts",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports  && cp -r ./src/public/ ./dist/",
    "heroku-prebuild": "npm build",
    "test": "jest",
    "test:watch": "jest --watchAll --silent",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "migrate:dev": "prisma migrate dev --name init"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "aws-sdk": "^2.1365.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "swagger-ui-express": "^4.6.3",
    "tzdata": "^1.0.36",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.165",
    "@swc/jest": "^0.2.20",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.2",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^1.4.1",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.190",
    "@types/luxon": "^2.3.1",
    "@types/node-cron": "^3.0.6",
    "@types/nodemailer": "^6.4.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/xlsx": "^0.0.35",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.0.0",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^4.7.1",
    "sucrase": "^3.21.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
